{
  "type": "module",
  "name": "ticket-system-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve-local": "kill-port 8261 && vite --mode locala --force",
    "serve-dev": "kill-port 8261 && vite --mode development --force",
    "serve-test": "kill-port 8261 && vite --mode testing --force",
    "serve-prod": "kill-port 8261 && vite --mode production --force",
    "build-local": "vite build --mode locala",
    "build-dev": "vite build --mode development",
    "build-test": "vite build --mode testing",
    "build-prod": "vite build --mode production",
    "kill-port": "kill-port 8261",
    "-------------------分界线---------------------": "",
    "lint": "eslint --ext .vue,.js,.jsx,.ts,.tsx src --fix",
    "lint2": "eslint --ext .vue,.js,.jsx,.ts,.tsx src/components --fix",
    "format": "prettier --write \"**/*.js\"",
    "format-vue": "prettier --write \"**/*.vue\"",
    "only-commit": "eslint --ext .js,.jsx,.ts,.tsx src && cz",
    "commit": "eslint --ext .js,.jsx,.ts,.tsx src && git add -A && cz",
    "install-force": "npm ci --force"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@stomp/stompjs": "^7.1.1",
