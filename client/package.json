    "@vueuse/core": "^10.11.1",
    "@vueuse/head": "^0.9.8",
    "@wangeditor-next/editor": "^5.6.34",
    "@wangeditor-next/editor-for-vue": "^5.1.14",
    "@wangeditor-next/plugin-mention": "^1.0.4",
    "@wangeditor/plugin-mention": "^1.0.0",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "element-plus": "^2.10.2",
    "eruda": "^3.4.3",
    "file-saver": "^2.0.5",
    "font-awesome": "^4.7.0",
    "image-conversion": "^2.1.1",
    "js-base64": "^3.7.7",
    "js-cookie": "^3.0.5",
    "jszip": "^3.10.1",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.9.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate": "^3.2.3",
    "postcss-pxtorem": "^6.1.0",
    "query-string": "^9.2.1",
    "uuid": "^11.1.0",
